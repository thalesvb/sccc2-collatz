{
    "opts": {
        "destination": "./docs/",
        "package": "package.json",
        "recurse": true,
        "readme": "README.md"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "default": {
            "includeDate": false,
            "outputSourceFiles": false
        }
    }
}